{
  "name": "dynasty_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npx prisma generate && npx prisma migrate deploy && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:mig": "npx prisma migrate dev --name 'init' && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "npx prisma migrate deploy &&  npm run start:prod",
    "start:migrate:production": "npx prisma migrate deploy &&  node dist/main",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev --name 'init'",
    "prisma:generate": "npx prisma generate",
    "docker:up": "docker-compose up",
    "docker:build": "docker-compose up --build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.9.1",
    "@casl/ability": "^6.3.3",
